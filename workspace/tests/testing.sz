container Test {

    task<number> add(a: number, b: number) = a + b
    task<number> add(a: number, b: number, c: number) = a + b + c

    task<boolean> match(a: boolean, b: boolean) = a && b

}

container Other

task<boolean> isinstance(value: any, cont: container) = value is cont

task<int> main {
    const a = Test()

    std::println(a::add(2.5, 2, 1))
    std::println(a::match(true, false))

    std::println(isinstance(a, Test))
    std::println(isinstance(a, Other))
}